@model AutoPartsHub.Models.TblContectU@{    ViewData["Title"] = "ContectUS";
    Layout = "~/Views/Shared/_Layout.cshtml";}<style>    .contact-row{
        box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;
        padding:3rem;
    }
    .contact-info-item{
        box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px !important;
    }
</style><!--== Start Page Header Area ==--><div class="page-header-wrap bg-img" data-bg="/assets/img/bg/page-header-bg.jpg">    <div class="container">        <div class="row">            <div class="col-12 text-center">                <div class="page-header-content">                    <div class="page-header-content-inner">                        <h1>Contact</h1>                        <ul class="breadcrumb">                            <li><a href="/">Home</a></li>                            <li class="current"><a href="#">Contact</a></li>                        </ul>                    </div>                </div>            </div>        </div>    </div></div><!--== End Page Header Area ==--><!--== Start Page Content Wrapper ==--><div class="page-content-wrapper sm-top">    <div class="contact-page-content">        <div class="contact-info-wrapper">            <div class="container">                <div class="row mtn-30">                    <div class="col-sm-6 col-lg-4">                        <div class="contact-info-item">                            <div class="con-info-icon">                                <i class="ion-ios-location-outline"></i>                            </div>                            <div class="con-info-txt">                                <h4>Our Location</h4>                                <p>                                    (800) 123 456 789 / (800) 123 456 789                                    mailto:info@example.com                                </p>                            </div>                        </div>                    </div>                    <div class="col-sm-6 col-lg-4">                        <div class="contact-info-item">                            <div class="con-info-icon">                                <i class="ion-iphone"></i>                            </div>                            <div class="con-info-txt">                                <h4>Contact us Anytime</h4>                                <p>                                    Mobile: 03369440406 <br>                                    Fax: 201201                                </p>                            </div>                        </div>                    </div>                    <div class="col-sm-6 col-lg-4">                        <div class="contact-info-item">                            <div class="con-info-icon">                                <i class="ion-ios-email-outline"></i>                            </div>                            <div class="con-info-txt">                                <h4>Write Some Words</h4>                                <p>                                    mailto:support24/7/hamzazia189131@gmail.com                                    mailto:info@example.com                                </p>                            </div>                        </div>                    </div>                </div>            </div>        </div>        <div class="contact-form-wrapper sm-top">            <div class="container">                <div class="row contact-row" >                    <div class="col-12">                        <div class="contact-form-content">                            <h2>Get In Touch</h2>                            <div class="contact-form-wrap">                                <form asp-controller="home" asp-action="contactus" method="post" id="contact-form">                                    <div class="contact-form-inner">                                        <div class="row">                                            <div class="col-md-6">                                                <div class="input-item">                                                    <label class="sr-only" for="name">name</label>                                                    <input type="text" class="form-control" asp-for="ContectUsName" id="ContectUsName" placeholder="Name" required />                                                </div>                                            </div>                                            <div class="col-md-6">                                                <div class="input-item">                                                    <label class="sr-only" for="email">email</label>                                                    <input type="email" class="form-control" id="email" asp-for="ContectUsEmail" placeholder="Email" required />                                                </div>                                            </div>                                            <div class="col-6">                                                <div class="input-item">                                                    <label class="sr-only" for="subject">subject</label>                                                    <input type="text" id="subject" asp-for="ContectUsSubject" placeholder="Subject" required />                                                </div>                                            </div>                                            <div class="col-6">                                                <div class="input-item">                                                    <label class="sr-only" for="subject">Phone No</label>                                                    <input type="text" id="ContectUsPhoneNo" asp-for="ContectUsPhoneNo" placeholder="Contect Us Phone No" required />                                                </div>                                            </div>                                            <div class="col-12">                                                <div class="input-item">                                                    <label class="sr-only" for="message">message</label>                                                    <textarea id="message" class="form-control" asp-for="ContectUsMassage" cols="30" rows="8" placeholder="Write Message" required></textarea>                                                </div>                                            </div>                                            <div class="col-12">                                                <div class="input-item">                                                    <button type="submit" class="btn btn-success btnf-r"><i class="fa fa-save"></i> Save</button>                                                </div>                                            </div>                                        </div>                                    </div>                                    @*                                    <!-- Form Submission Notification -->                                    <div class="form-message"></div> *@                                </form>                            </div>                        </div>                    </div>                </div>            </div>        </div>        @* <div class="contact-map-wrapper sm-top">        <div id="map_content" class="h-100" data-lat="23.7639212" data-lng="90.429871" data-zoom="7"></div>        </div> *@        <!DOCTYPE html>        <html>        <head>            <title>Map of Pakistan</title>            <meta charset="utf-8" />            <meta name="viewport" content="width=device-width, initial-scale=1.0">            <link rel="stylesheet" href="https://unpkg.com/leaflet@1.7.1/dist/leaflet.css" />            <style>                #map_content {                    height: 100%;                }                .contact-map-wrapper {                    height: 500px;                }            </style>        </head>        <body>            <div class="contact-map-wrapper sm-top">                <div id="map_content" class="h-100" data-lat="30.3753" data-lng="69.3451" data-zoom="6"></div>            </div>            <script src="https://unpkg.com/leaflet@1.7.1/dist/leaflet.js"></script>            <script>                document.addEventListener("DOMContentLoaded", function () {                    var mapElement = document.getElementById('map_content');                    var lat = mapElement.getAttribute('data-lat');                    var lng = mapElement.getAttribute('data-lng');                    var zoom = mapElement.getAttribute('data-zoom');                    var map = L.map('map_content').setView([lat, lng], zoom);                    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {                        maxZoom: 19                    }).addTo(map);                });            </script>        </body>    </html></div></div><!--== End Page Content Wrapper ==-->